{
    "llm": {
        "temperature": 0.7,
        "providers": {
            "litellm": {
                "use": true,
                "model": "openai/gpt-4o",
                "fallback_models": [],
                "cache_responses": false
            },
            "openrouter": {
                "use": false,
                "model": "deepseek/deepseek-r1:free",
                "fallback_models": [],
                "timeout": 60,
                "use_openai_client": true
            },
            "max_tokens": 8192
        }
    },
    "context_window": 10,
    "max_steps": 300,
    "environment": {
        "working_dir": "workspace",
        "output_dir": "output"
    },
    "memory": {
        "memory_path": "workspace/memory"
    },
    "arxiv": {
        "pdf_dir": "workspace/papers"
    },
    "agent": {
        "agent_id": "scientist",
        "task": null,
        "paper_id": "all-in-one",
        "paper_path": "debug/data/all-in-one",
        "debug": false
    },
    "output_file": null,
    "system_prompt_template": "system_prompt.j2",
    "user_prompt_template": "user_prompt.j2",
    "max_message_chars": 30000
}