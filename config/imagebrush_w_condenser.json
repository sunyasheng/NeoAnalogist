{
    "llm": {
        "temperature": 0.7,
        "providers": {
            "litellm": {
                "use": true,
                "model": "openai/gpt-5-mini",
                "fallback_models": [],
                "cache_responses": false
            },
            "openrouter": {
                "use": false,
                "model": "deepseek/deepseek-r1:free",
                "fallback_models": [],
                "timeout": 60,
                "use_openai_client": true
            },
            "max_tokens": 8192
        }
    },
    "context_window": 10,
    "max_steps": 300,
    "environment": {
        "working_dir": "workspace",
        "output_dir": "output"
    },
    "memory": {
        "memory_path": "workspace/memory"
    },
    "agent": {
        "agent_id": "scientist",
        "task": null,
        "debug": false
    },
    "env_vars": {
        "QWEN_API_URL": "http://10.109.28.79:8200"
    },
    "condenser": {
        "type": "pipeline",
        "condensers": [
            {
                "type": "browser_output",
                "attention_window": 1
            },
            {
                "type": "llm_summarizing",
                "max_size": 200,
                "keep_first": 5,
                "max_event_length": 15000
            }
        ]
    },
    "output_file": null,
    "system_prompt_template": "image_manipulation_system_prompt.j2",
    "user_prompt_template": "user_prompt.j2",
    "max_message_chars": 30000
}


